<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/myTickets.css" />
    <title>Meus Bilhetes</title>
  </head>
  <body>
    <main>
      <section class="topBar">
        <a href="/">Home</a>
        <h1>Escolha o Evento</h1>
        <form action="/auth/logout" method="post">
          <input type="submit" value="SAIR" />
        </form>
      </section>

      <table class="tableStyle"><thead class="tablehead">
          <tr>
            <th>Evento</th>
            <th>Descrição</th>
            <th>Endereço</th>
            <th>data</th>
          </tr></thead>
        <tbody>
          {{#each data}}
            <a href="/events/{{_id}}">
              <tr>
                <td class="cell">{{EventName}}</td>
                <td class="cell">{{EventDescription}}</td>
                <td class="cell">{{EventAddress}}</td>
                <td class="cell">{{EventDate}}</td>
              </tr>
            </a>
          {{/each}}
        </tbody>
      </table>

    </main>
  </body>
</html>